#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/lopatoj/Projects/website/node_modules/.pnpm/@sveltejs+kit@2.15.2_@sveltejs+vite-plugin-svelte@4.0.4_svelte@5.16.6_vite@5.4.11_@type_4b99ae3e11ae235dae939ffc45d46dcd/node_modules/@sveltejs/kit/node_modules:/home/lopatoj/Projects/website/node_modules/.pnpm/@sveltejs+kit@2.15.2_@sveltejs+vite-plugin-svelte@4.0.4_svelte@5.16.6_vite@5.4.11_@type_4b99ae3e11ae235dae939ffc45d46dcd/node_modules/@sveltejs/node_modules:/home/lopatoj/Projects/website/node_modules/.pnpm/@sveltejs+kit@2.15.2_@sveltejs+vite-plugin-svelte@4.0.4_svelte@5.16.6_vite@5.4.11_@type_4b99ae3e11ae235dae939ffc45d46dcd/node_modules:/home/lopatoj/Projects/website/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/lopatoj/Projects/website/node_modules/.pnpm/@sveltejs+kit@2.15.2_@sveltejs+vite-plugin-svelte@4.0.4_svelte@5.16.6_vite@5.4.11_@type_4b99ae3e11ae235dae939ffc45d46dcd/node_modules/@sveltejs/kit/node_modules:/home/lopatoj/Projects/website/node_modules/.pnpm/@sveltejs+kit@2.15.2_@sveltejs+vite-plugin-svelte@4.0.4_svelte@5.16.6_vite@5.4.11_@type_4b99ae3e11ae235dae939ffc45d46dcd/node_modules/@sveltejs/node_modules:/home/lopatoj/Projects/website/node_modules/.pnpm/@sveltejs+kit@2.15.2_@sveltejs+vite-plugin-svelte@4.0.4_svelte@5.16.6_vite@5.4.11_@type_4b99ae3e11ae235dae939ffc45d46dcd/node_modules:/home/lopatoj/Projects/website/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../svelte-kit.js" "$@"
else
  exec node  "$basedir/../../svelte-kit.js" "$@"
fi

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/lopatoj/Projects/website/node_modules/.pnpm/sanity@3.68.3_@emotion+is-prop-valid@1.2.2_@types+babel__core@7.20.5_@types+node@22.10._0086519d9a7bb6a1f4ca8ef668102d99/node_modules/sanity/bin/node_modules:/home/lopatoj/Projects/website/node_modules/.pnpm/sanity@3.68.3_@emotion+is-prop-valid@1.2.2_@types+babel__core@7.20.5_@types+node@22.10._0086519d9a7bb6a1f4ca8ef668102d99/node_modules/sanity/node_modules:/home/lopatoj/Projects/website/node_modules/.pnpm/sanity@3.68.3_@emotion+is-prop-valid@1.2.2_@types+babel__core@7.20.5_@types+node@22.10._0086519d9a7bb6a1f4ca8ef668102d99/node_modules:/home/lopatoj/Projects/website/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/lopatoj/Projects/website/node_modules/.pnpm/sanity@3.68.3_@emotion+is-prop-valid@1.2.2_@types+babel__core@7.20.5_@types+node@22.10._0086519d9a7bb6a1f4ca8ef668102d99/node_modules/sanity/bin/node_modules:/home/lopatoj/Projects/website/node_modules/.pnpm/sanity@3.68.3_@emotion+is-prop-valid@1.2.2_@types+babel__core@7.20.5_@types+node@22.10._0086519d9a7bb6a1f4ca8ef668102d99/node_modules/sanity/node_modules:/home/lopatoj/Projects/website/node_modules/.pnpm/sanity@3.68.3_@emotion+is-prop-valid@1.2.2_@types+babel__core@7.20.5_@types+node@22.10._0086519d9a7bb6a1f4ca8ef668102d99/node_modules:/home/lopatoj/Projects/website/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/sanity" "$@"
else
  exec node  "$basedir/../../bin/sanity" "$@"
fi
